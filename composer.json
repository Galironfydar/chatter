{
    "name": "devdojo/chatter",
    "description": "A simple forum package for your laravel app",
    "authors": [
        {
            "name": "Tony Lea",
            "email": "tony@devdojo.com"
        }
    ],
    "repositories": {
        "purifier": {
            "type": "vcs",
            "url": "https://github.com/Galironfydar/Purifier.git"
        }
    },
    "require": {
        "luketowers/purifier": "dev-master"
    },
    "require-dev": {
        "illuminate/cache": "^7.0",
        "illuminate/console": "^7.0",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "~5.7.14"
    },
    "autoload": {
	    "psr-4": {
	      "DevDojo\\Chatter\\": "/src"
		}
	},
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevDojo\\Chatter\\ChatterServiceProvider"
            ]
        }
    }
}
